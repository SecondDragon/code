    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.56",
